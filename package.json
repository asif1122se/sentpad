    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "@fontsource/inter": "^4.5.12",
    "@tanstack/react-query": "^4.2.3",
    "@tanstack/react-table": "^8.5.11",
    "add": "^2.0.6",
    "axios": "^1.1.3",
    "date-fns-tz": "^1.3.7",
    "easy-peasy": "^5.0.4",
    "formik": "^2.2.9",
    "framer-motion": "^6",
    "grapesjs": "^0.20.1",
    "grapesjs-plugin-ckeditor": "^0.0.10",
    "grapesjs-preset-newsletter": "^0.2.21",
    "juice": "^8.1.0",
    "react": "^18.2.0",
    "react-color": "^2.19.3",
    "react-csv": "^2.2.2",
